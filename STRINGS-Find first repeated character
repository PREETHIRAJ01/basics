code:
#include <stdio.h>

int main()
{
	int n,i;
	scanf("%d",&n);
	int found=0;
	while(n--)
	{
	    char s[10000];
	    scanf("%s",s);
	    int a[27]={0};// alphabet frequency array 
	    int l=strlen(s);
	    found=0;
	    for(i=0;i<strlen(s);i++)
	    {
            int k=s[i]-96;
            a[k]++;
            if(a[k]==2)
            {
                found=1;
                printf("%c",s[i]);
                break;
            }
	    }
	    if(found==0)
	    {
	        printf("-1");
	    }
	    
	    printf("\n");
	}
	return 0;
}
or
int main()
{
	int t,i,j,k,l,n,m;
	cin>>t;
	while(t--)
	{string s;
	    cin>>s;
	    int a[26]={0};l=0;
	    for(i=0;i<s.size();i++)
	    {
	        a[s[i]-'a']++;--> this sets from 0 to 26
	        if(a[s[i]-'a']==2)
	        {
	            l=1;
	            cout<<s[i]<<endl;
	            break;
	        }
	    }
	    if(!l)
	        cout<<"-1\n";
	}
	return 0;
}
