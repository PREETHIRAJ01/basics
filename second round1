

question: to find the square numbers or perfect numbers 










#include <stdio.h>

int main()
{
    int a,b,i,j;
    scanf("%d%d",&a,&b);
    for(i=a;i<=b;i++)
    {
        for(j=1;(j*j)<=i;j++)
        {
            if((j*j)==i)
            {
                printf("  %d",j*j);
            }
        }
    }

    return 0;
}
